{ "extends": ["react-app", "react-app/jest", "plugin:prettier/recommended"],
  "rules": {
    "prettier/prettier": ["error",
      {
        "singleQuote": true,
        "printWidth": 100,
        "semi": true,
        "jsxSingleQuote": true,
        "arrowParens": "avoid"
  }]},
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}